<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uk.ac.cam.cl.group_project.delta.simulation.gui.Controller">
   <children>
      <SplitPane dividerPositions="0.3" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <items>
            <AnchorPane>
               <children>
                  <TabPane prefHeight="400.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <tabs>
                      <Tab text="Properties">
                           <content>
                              <AnchorPane>
                                 <children>
                                    <TreeView fx:id="propertiesView" prefHeight="367.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                 </children>
                              </AnchorPane>
                           </content>
                      </Tab>
                    </tabs>
                  </TabPane>
               </children>
            </AnchorPane>
            <Pane fx:id="viewPane" onMouseClicked="#onViewPaneMouseClick" onMouseDragged="#onViewPaneMouseDragged" onMousePressed="#onViewPaneMousePressed" onScroll="#onViewPaneScroll" scaleY="-1.0">
               <children>
                  <Group fx:id="scene">
                     <children>
                        <Line endX="10.0" startX="-10.0" />
                        <Line endY="10.0" startY="-10.0" />
                     </children></Group>
               </children>
				<cursor>
					<Cursor fx:constant="OPEN_HAND" />
				</cursor>
            </Pane>
         </items>
      </SplitPane>
   </children>
</AnchorPane>
